{
  "blocks": "never",
  "brackets": false,
  "colons": "always",
  "commaSpace": "always",
  "commentSpace": "always",
  "cssLiteral": "never",
  "duplicates": true,
  "efficient": "always",
  "extendPref": "@extend",
  "groupOutputByFile": true,
  "indentPref": 2,
  "leadingZero": "always",
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": true,
  "parenSpace": "never",
  "prefixVarsWithDollar": "never",
  "quotePref": "double",
  "semicolons": "never",
  "sortOrder": false,
  "trailingWhitespace": "never",
  "universal": "never",
  "valid": true,
  "zeroUnits": "never"
}
